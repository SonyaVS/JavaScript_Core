var login = prompt('Who\'s there?');

if (login == 'Cancel'){
        alert('Canceled');
}
if (login == 'Other'){
        alert('I don\'t know you');
} 
if (login == 'Admin'){
        alert('Password');
}

var password = prompt ('Password?');
if (password == 'Cancel'){
        alert('Canceled');
} 
if (password == 'Other'){
        alert('Wrong password');
} 
if (password == 'TheMaster'){
        alert('Welcome!');
}
